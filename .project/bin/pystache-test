#!/home/tulio/project/.project/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pystache==0.5.3','console_scripts','pystache-test'
__requires__ = 'pystache==0.5.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pystache==0.5.3', 'console_scripts', 'pystache-test')()
    )
